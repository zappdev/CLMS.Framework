<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" default-lazy="true" auto-import="false" assembly="zAppDev.DotNet.Framework" namespace="zAppDev.DotNet.Framework.Identity.Model">
    <class name="ApplicationUserLogin" table="`ApplicationUserLogins`" schema="`security`">
        <cache usage="read-write" region="security" />
        <id name="Id" column="`Id`" type="System.Int32">
            <generator class="NHibernate.Id.Enhanced.TableGenerator">
                <param name="increment_size">200</param>
                <param name="segment_value">ApplicationUserLogins</param>
                <param name="optimizer">pooled-lo</param>
            </generator>
        </id>
        <property name="LoginProvider" column="`LoginProvider`" type="String" not-null="false" length="128" />
        <property name="ProviderKey" column="`ProviderKey`" type="String" not-null="false" length="128" />
        <many-to-one name="User" access="field.camelcase" class="ApplicationUser" column="`User`" cascade="persist,merge,save-update" foreign-key="FK_ApplicationUserLogins_To_ApplicationUsers_On_User" />
    </class>
</hibernate-mapping>